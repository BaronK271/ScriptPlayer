<Window x:Class="ScriptPlayer.VideoSync.Dialogs.VisualTrackerDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScriptPlayer.VideoSync.Dialogs"
        mc:Ignorable="d"
        Title="VisualTrackerDialog" Height="300" Width="300">
    <Grid>
        <Viewbox Margin="10">
            <Grid Name="Preview">
                <Rectangle Name="Center" Fill="#99ff0000" Height="10" Stroke="Black" Width="10" HorizontalAlignment="Left" VerticalAlignment="Top"/>
            </Grid>
        </Viewbox>
    </Grid>
</Window>
